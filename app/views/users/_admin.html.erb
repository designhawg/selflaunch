<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>City</th>
    <th>Country</th>
    <th>State</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.city_id %></td>
    <td><%= user.country_id %></td>
    <td><%= user.state_id %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

